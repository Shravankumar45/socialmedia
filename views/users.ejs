<div class="container-fluid">

    <div class="row">
        <% users.forEach(function(users) {%>
        <div class="col-sm-4 ">
                <div class="card" style="width: 18rem;">
                        <img src="<%= users.image %>" class="card-img-top">
                        <div class="card-body">
                          <h5 class="card-title"><%= users.fullname %></h5>
                          <p id="address" class="card-title"><%= users.location %></p>
                        </div>
                      </div>

        </div>
        <% }); %>
       
    </div>
   
</div>